<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 填充 -->
    <solid android:color="@android:color/white" />

    <!-- 设置圆角半径 -->
    <corners android:radius="@dimen/y_bg_et_corners_radius" />
    
    <!-- 各方向的间隔 -->
    <padding
        android:bottom="@dimen/y_bg_et_padding_top_bottom"
        android:left="@dimen/y_bg_et_padding_left_right"
        android:right="@dimen/y_bg_et_padding_left_right"
        android:top="@dimen/y_bg_et_padding_top_bottom" />
    
    <!-- 描边 -->
    <stroke
        android:width="@dimen/y_bg_et_stroke_width"
        android:color="@android:color/darker_gray" />

</shape>